{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "dest": "/index.js"
        }
    ],
    "env": {
        "CORS_ORIGIN": "https://employee-vacations-system.vercel.app"
    }
}
